{
	"name": "Metadata sync pipeline",
	"properties": {
		"activities": [
			{
				"name": "Assure table Temp_CIS_Metadata_Job clean",
				"type": "AzureDataExplorerCommand",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"command": ".clear table Temp_CIS_Metadata_Job data",
					"commandTimeout": "00:20:00"
				},
				"linkedServiceName": {
					"referenceName": "M365OpsCenterInt",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Swap CIS_Metadata_Job and Temp_CIS_Metadata_Job",
				"type": "AzureDataExplorerCommand",
				"dependsOn": [
					{
						"activity": "Copy Job from Sql to Kusto",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"command": ".rename tables CIS_Metadata_Job=Temp_CIS_Metadata_Job ifexists, Temp_CIS_Metadata_Job=CIS_Metadata_Job",
					"commandTimeout": "00:20:00"
				},
				"linkedServiceName": {
					"referenceName": "M365OpsCenterInt",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Clear table Temp_CIS_Metadata_Job",
				"type": "AzureDataExplorerCommand",
				"dependsOn": [
					{
						"activity": "Swap CIS_Metadata_Job and Temp_CIS_Metadata_Job",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"command": ".clear table Temp_CIS_Metadata_Job data",
					"commandTimeout": "00:20:00"
				},
				"linkedServiceName": {
					"referenceName": "M365OpsCenterInt",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Copy Job from Sql to Kusto",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Assure table Temp_CIS_Metadata_Job clean",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "select * from dbo.Job where JobType='M365AGCBuildout'",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureDataExplorerSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Sql_Job",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Kusto_Temp_Job",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}