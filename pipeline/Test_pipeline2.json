{
	"name": "Test_pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Copy Job from REST API to Kusto",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "AzureDataExplorerSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['Id']"
								},
								"sink": {
									"name": "JobId"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "CisBeta_REST_Job",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Kusto_INT_Temp_Job",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}