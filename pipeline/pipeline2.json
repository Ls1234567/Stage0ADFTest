{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Copy Task from OData to Kusto",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "AzureDataExplorerSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['TaskId']"
								},
								"sink": {
									"name": "TaskId"
								}
							},
							{
								"source": {
									"path": "['JobId']"
								},
								"sink": {
									"name": "JobId"
								}
							},
							{
								"source": {
									"path": "['TaskName']"
								},
								"sink": {
									"name": "TaskName"
								}
							},
							{
								"source": {
									"path": "['DisplayName']"
								},
								"sink": {
									"name": "DisplayName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['ExternalId']"
								},
								"sink": {
									"name": "ExternalId"
								}
							},
							{
								"source": {
									"path": "['ExternalSystemType']"
								},
								"sink": {
									"name": "ExternalSystemType"
								}
							},
							{
								"source": {
									"path": "['Component']"
								},
								"sink": {
									"name": "Component"
								}
							},
							{
								"source": {
									"path": "['OrderId']"
								},
								"sink": {
									"name": "OrderId"
								}
							},
							{
								"source": {
									"path": "['CreatedDate']"
								},
								"sink": {
									"name": "CreatedDate"
								}
							},
							{
								"source": {
									"path": "['StartDate']"
								},
								"sink": {
									"name": "StartDate",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "['FinishDate']"
								},
								"sink": {
									"name": "FinishDate",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "['StateName']"
								},
								"sink": {
									"name": "StateName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['CustomState']"
								},
								"sink": {
									"name": "CustomState"
								}
							},
							{
								"source": {
									"path": "['TaskType']"
								},
								"sink": {
									"name": "TaskType"
								}
							},
							{
								"source": {
									"path": "['AssignedTo']"
								},
								"sink": {
									"name": "AssignedTo"
								}
							},
							{
								"source": {
									"path": "['Timestamp']"
								},
								"sink": {
									"name": "Timestamp",
									"type": "DateTime"
								}
							}
						],
						"collectionReference": "$['value']",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "ODataTask",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Kusto_Temp_Task",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}