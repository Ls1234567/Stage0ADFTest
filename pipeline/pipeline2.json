{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "AzureDataExplorerSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['value'][0]['JobId']"
								},
								"sink": {
									"name": "JobId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['ParentId']"
								},
								"sink": {
									"name": "ParentId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['RootId']"
								},
								"sink": {
									"name": "RootId",
									"type": "String"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "ODataJob",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Kusto_Temp_Job",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}